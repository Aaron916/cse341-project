<!DOCTYPE html>
<html>

<head>
  <% include ../includes/header.ejs %>
</head>

<body>
  <% include ../includes/nav.ejs %>
    <div class="jumbotron text-center">
      <div class="container">
        <h1>
          <%= title %>
        </h1>
        <p>This is the placeholder for Team Activity 03</p>
      </div>
    </div>
    <div class="container">
      <% for (let i of items ) {%>
        <div>
          <p><%=i.name%></p>
          <p>Price: <%= i.price %></p>
          <p>Description <%= i.description %></p>
          <img src="<%= i.imageUrl %>"></img>
        </div>

      <% } %>
    </div>


</body>

</html>